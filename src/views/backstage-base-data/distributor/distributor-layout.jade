extends ../base-data-layout

block subScripts
    link(href=relativeRoot + "css/components/lightbox.css", rel="stylesheet", type="text/css")

block side-modules
    ul.feature_nav
        each tab in backstageTops[0].sides[3].tabs
            li
                a(href=tab.link) #{tab.name}
    .main_content
        block content